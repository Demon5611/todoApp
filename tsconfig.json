{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true
    },
    "include": ["src"]
  }
  